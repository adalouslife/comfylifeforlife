{
  "title": "Comfy Faceswap Worker (GPU)",
  "description": "Minimal ComfyUI serverless worker with health checks and URL-based swap op. Optimized for quick publish tests.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://raw.githubusercontent.com/comfyanonymous/ComfyUI/master/comfyui.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 40,
    "presets": [
      {
        "name": "Default",
        "defaults": {
          "COMFY_MODE": "production",
          "INPUT_DIR": "/workspace/ComfyUI/input",
          "OUTPUT_DIR": "/workspace/ComfyUI/output"
        }
      }
    ],
    "env": [
      {
        "key": "COMFY_MODE",
        "value": "production"
      },
      {
        "key": "INPUT_DIR",
        "value": "/workspace/ComfyUI/input"
      },
      {
        "key": "OUTPUT_DIR",
        "value": "/workspace/ComfyUI/output"
      }
    ],
    "allowedCudaVersions": [
      "12.7",
      "12.6",
      "12.5",
      "12.4",
      "12.3",
      "12.2",
      "12.1",
      "12.0"
    ]
  }
}
