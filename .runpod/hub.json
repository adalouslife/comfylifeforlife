{
  "title": "ComfyLife Serverless â€” Smoke GPU",
  "description": "Minimal GPU smoke endpoint for Hub validation; skips workflows during tests.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://raw.githubusercontent.com/runpod/runpodctl/main/assets/runpod-logo.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 16,
    "presets": [
      {
        "name": "Smoke Test",
        "defaults": {
          "COMFY_MODE": "production",
          "OUTPUT_DIR": "/workspace/outputs",
          "INPUT_DIR": "/workspace/inputs",
          "RP_HEALTH_ONLY": "true"
        }
      }
    ],
    "env": [
      { "key": "RUNPOD_TEST_MODE", "value": "true" },
      { "key": "RP_HEALTH_ONLY", "value": "true" },
      { "key": "COMFY_MODE", "value": "production" },
      { "key": "OUTPUT_DIR", "value": "/workspace/outputs" },
      { "key": "INPUT_DIR", "value": "/workspace/inputs" },
      {
        "key": "PUBLIC_CALLBACK_BASE",
        "input": {
          "name": "Callback Base URL",
          "type": "string",
          "description": "Leave empty for tests; used in real runs to host returned files.",
          "default": ""
        }
      }
    ]
  }
}
