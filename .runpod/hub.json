{
  "title": "Comfylifeforlife (Serverless Faceswap)",
  "description": "ComfyUI on RunPod Serverless. Accepts image URLs, performs faceswap via a ComfyUI workflow, returns a public image URL.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://raw.githubusercontent.com/comfyanonymous/ComfyUI/master/comfyui.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 40,
    "presets": [
      {
        "name": "Default",
        "defaults": {
          "COMFY_HOST": "127.0.0.1",
          "COMFY_PORT": "8188",
          "WORKFLOW_PATH": "/workspace/comfyui/workflows/APIAutoFaceACE.json",
          "INPUT_DIR": "/workspace/inputs",
          "OUTPUT_DIR": "/workspace/outputs",
          "UPLOAD_PROVIDER": "catbox"
        }
      }
    ],
    "env": [
      { "key": "COMFY_HOST", "value": "127.0.0.1" },
      { "key": "COMFY_PORT", "value": "8188" },
      { "key": "WORKFLOW_PATH", "value": "/workspace/comfyui/workflows/APIAutoFaceACE.json" },
      { "key": "INPUT_DIR", "value": "/workspace/inputs" },
      { "key": "OUTPUT_DIR", "value": "/workspace/outputs" },
      { "key": "UPLOAD_PROVIDER", "value": "catbox" }
    ]
  }
}
