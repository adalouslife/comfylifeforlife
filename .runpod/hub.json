{
  "title": "Comfy Faceswap Worker",
  "description": "Minimal ComfyUI worker on RunPod Serverless. Health check + ping. Workflow support will be layered later.",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://raw.githubusercontent.com/comfyanonymous/ComfyUI/master/comfyui.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 40,
    "presets": [
      {
        "name": "Default",
        "defaults": {
          "COMFY_MODE": "production"
        }
      }
    ],
    "env": [
      {
        "key": "COMFY_MODE",
        "value": "production"
      },
      {
        "key": "RP_HANDLER_PORT",
        "value": "8000"
      }
    ]
  }
}
